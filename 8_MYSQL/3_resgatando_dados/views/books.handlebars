<h1>Livros disponíveis:</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">TÍTULO</th>
      <th scope="col">QTD PÁGINAS</th>
    </tr>
  </thead>
  <tbody>
    {{#each books}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{title}}</td>
            <td>{{pagesqty}}</td>
        </tr>
    {{/each}}
  </tbody>
</table>